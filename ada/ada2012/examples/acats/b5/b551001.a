         function Cnst_Index1 (Container : T1; Ind : Natural)
            return Natural is (Ind);

         function Iterate1 (Container : T1)
            return Some_Iterator.Forward_Iterator'Class;
         function Iterate1 (Container : T1)
            return Some_Concrete_Iterator'Class;

