with BA14001A.Daughter;
generic
   type Item is range <>;
function BA14001A.Daughter (Arg : in Item) return Item;   -- ERROR: {3:1}

function BA14001A.Daughter (Arg : in Item) is
   return Item is     -- OPTIONAL ERROR: {1:1} No legal specification.
begin
   return Arg - 1;
end BA14001A.Daughter;

