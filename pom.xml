		<module>java8</module>
		<module>json</module>
		<module>less</module>
		<module>logo</module>
		<module>lua</module>
		<module>masm</module>
		<module>memcached_protocol</module>
		<module>MPS</module>
		<module>mumath</module>
		<module>mumps</module>
		<module>mysql</module>
		<module>objc</module>
		<module>pascal</module>
		<module>pcre</module>
		<module>peoplecode</module>
		<module>pgn</module>
		<!--
		<module>php</module>
	-->
		<module>propcalc</module>
		<module>r</module>
		<module>redcode</module>
		<module>scala</module>
		<module>scss</module>
		<module>smalltalk</module>
		<module>sparql</module>
		<module>sqlite</module>
		<module>stacktrace</module>
