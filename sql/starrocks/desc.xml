<?xml version="1.0" encoding="UTF-8" ?>
<desc xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../_scripts/desc.xsd">

    <!-- antlr-version 4.10 required because caseinsensitive is used in
    the grammar. -->
    <antlr-version>^4.10</antlr-version>

    <!-- entry-point required because there are multiple start symbols
    possible. -->
    <entry-point>sqlStatements</entry-point>

    <!-- Works only with Java because of symbol conflicts. -->
    <targets>Java</targets>
</desc>